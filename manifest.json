{
  "manifest_version": 3,
  "name": "Time Tracker",
  "version": "1.0",
  "description": "Track time spent on websites and analyze productivity.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon.png"
  }
}
